<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <groupId>org.matsim</groupId>
    <artifactId>playgrounds</artifactId>
    <version>0.2.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>playground</groupId>
  <artifactId>johannes</artifactId>
  <name>johannes</name>
  <version>0.2.0-SNAPSHOT</version>
  <build>
  	<plugins>
  		<plugin>
  			<groupId>org.apache.maven.plugins</groupId>
  			<artifactId>maven-surefire-plugin</artifactId>
  			<version>2.4</version>
        <configuration>
          <excludes>
            <exclude>playground/johannes/gis/ZoneLayerSHPTest.java</exclude>
          </excludes>
        </configuration>
  		</plugin>
  	</plugins>
  </build>
  <repositories>
  	<repository>
  		<id>geotools</id>
  		<name>Geotools repository</name>
  		<url>http://maven.geotools.fr/repository</url>
  	</repository>
  </repositories>
  <dependencies>
  	<dependency>
  		<groupId>org.matsim.contrib</groupId>
  		<artifactId>sna</artifactId>
  		<version>0.2.0-SNAPSHOT</version>
  		<scope>compile</scope>
  	</dependency>
  	<dependency>
      <groupId>trove</groupId>
      <artifactId>trove</artifactId>
      <version>2.0.3</version>
    </dependency>
    <dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.2</version>
		</dependency>
    <dependency>
    	<groupId>org.geotools</groupId>
    	<artifactId>gt2-epsg-hsql</artifactId>
    	<version>2.3.5</version>
    </dependency>
  </dependencies>
</project>
