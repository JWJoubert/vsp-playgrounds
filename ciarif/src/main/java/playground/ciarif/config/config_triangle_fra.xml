<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.vsp.tu-berlin.de/projects/Matsim/data/dtd/config_v1.dtd"
	[
		<!ENTITY CVSROOT      "C:\Documents and Settings\ciarif/sandbox00">
		<!ENTITY PROJBASE     "vsp-cvs/devel/matsim/matsimJ">
		<!ENTITY STUDYBASE    "vsp-cvs/studies">
		<!ENTITY OUTPUTBASE   "vsp-cvs/devel/matsim/matsimJ/output">
		<!ENTITY HOMEPAGEBASE "http://www.vsp.tu-berlin.de/projects/Matsim">
		<!ENTITY LOCALDTDBASE "vsp-cvs/www/htdocs/FG4/projects/Matsim/data/dtd">
	]
>
<config xml:lang="de-CH">

<!-- ====================================================================== -->
<!-- triangle test                                                          -->
<!-- use org.matsim.TriangleTest as the main class                        -->
<!-- ====================================================================== -->

	<module name="config">
		<param name="outputConfigDTD" value="&HOMEPAGEBASE;/data/dtd/config_v1.dtd" />
		<param name="outputVersion" value="v1" />
		<param name="outputConfigFile" value="&CVSROOT;/&OUTPUTBASE;/output_config.xml" />
	</module> <!-- config -->

<!-- ====================================================================== -->

	<module name="global">
		<param name="randomSeed" value="4711" />
		<param name="outputTimeFormat" value="HH:mm:ss" />
	</module> <!-- global -->

<!-- ====================================================================== -->

	<module name="world">
		<param name="inputWorldFile" value="&CVSROOT;/&STUDYBASE;/triangle/world/world_cell20x20_blk_muni.xml" />
		<param name="localInputDTD" value="&CVSROOT;/&LOCALDTDBASE;/world_v2.dtd" />
		<param name="outputWorldFile" value="&CVSROOT;/&OUTPUTBASE;/output_world.xml" />
		<param name="outputWorldDTD" value="&HOMEPAGEBASE;/data/dtd/world_v2.dtd" />
		<param name="outputVersion" value="v2" />
	</module> <!-- world -->

<!-- ====================================================================== -->

	<module name="network">
		<param name="inputNetworkFile" value="&CVSROOT;/&STUDYBASE;/triangle/network/net.xml" />
		<param name="localInputDTD" value="&CVSROOT;/&LOCALDTDBASE;/network_v1.dtd" />
		<param name="outputNetworkFile" value="&CVSROOT;/&OUTPUTBASE;/output_network.xml" />
		<param name="outputNetworkDTD" value="&HOMEPAGEBASE;/data/dtd/network_v1.dtd" />
		<param name="outputVersion" value="v1" />
	</module> <!-- network -->

<!-- ====================================================================== -->

	<module name="facilities">
		<param name="inputFacilitiesFile" value="&CVSROOT;/&STUDYBASE;/triangle/test-facilities/facilities_v1.xml" />
		<param name="localInputDTD" value="&CVSROOT;/&LOCALDTDBASE;/facilities_v1.dtd" />
		<param name="outputFacilitiesFile" value="&CVSROOT;/&OUTPUTBASE;/output_facilities.xml" />
		<param name="outputFacilitiesDTD" value="&HOMEPAGEBASE;/data/dtd/facilities_v1.dtd" />
		<param name="outputVersion" value="v1" />
	</module> <!-- landuse -->

<!-- ====================================================================== -->

	<module name="plans">
		<param name="outputPlansFile" value="&CVSROOT;/&OUTPUTBASE;/output_plans.xml" />
		<param name="outputPlansDTD" value="&HOMEPAGEBASE;/data/dtd/plans_v4.dtd" />
		<param name="outputVersion" value="v4" />
		<param name="outputSample" value="1.00" />
		<param name="switchOffPlansStreaming" value="yes" />
	</module> <!-- plans -->

	<module name="plansCreateFromNetwork">
		<param name="person2CellsProportion" value="2" />
	</module> <!-- personCreateFromNetwork -->

<!-- ====================================================================== -->
<!-- ====================================================================== -->
<!-- ====================================================================== -->

</config>
