<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd">
<!-- Fairly minimalistic config file.  See config dump in console or in log file for more options, and some explanations. -->
<config>

	<module name="network">
		<param name="inputNetworkFile" value="./tnicolai/configs/network.xml" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="./tnicolai/configs/plans100.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="/Users/thomas/Development/workspace/matsim/output/example_i50_smix6" />
		<param name="eventsFileFormat" value="xml" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="50" />
		<param name="writeEventsInterval" value="0" />
	</module>

	<module name="planCalcScore" >
		<param name="activityType_0" value="h" />
		<param name="activityType_1" value="w" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
		<param name="activityTypicalDuration_1" value="08:00:00" />
	</module>

	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="5" /> <!-- 0 means unlimited -->

		<param name="ModuleProbability_1" value="0.9" />
		<param name="Module_1" value="ChangeExpBeta" />

		<param name="ModuleProbability_2" value="0.1" />
		<param name="Module_2" value="ReRoute_Dijkstra" />

		<param name="ModuleProbability_3" value="0.1" />
		<param name="Module_3" value="TimeAllocationMutator" />
	</module>
	
	<module name="JDEQSim">
		<param name="flowCapacityFactor" value="1.0"   />
		<param name="storageCapacityFactor" value="1.0"   />
	</module>
	
	<module name="parallelEventHandling">
        <param name="numberOfThreads" value="2" />
    </module>
	
</config>