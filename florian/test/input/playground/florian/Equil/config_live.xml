<?xml version="1.0" ?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v1.dtd">
<config>


	<module name="network">
		<param name="inputNetworkFile" value="./test/input/playground/florian/Equil/network.xml" />
	</module>

	<module name="plans">
		<param name="inputPlansFile" value="./test/input/playground/florian/Equil/plans100.xml" />
	</module>

	<module name="controler">
		<param name="outputDirectory" value="./Output/Equil/Output_live" />
		<param name="firstIteration" value="0" />
		<param name="lastIteration" value="1" />
	</module>

	<module name="qsim" >
		<param name="startTime" value="00:00:00" />
		<param name="endTime" value="00:00:00" />
		
		<param name="flowCapacityFactor" value="0.1" />
		<param name="storageCapacityFactor" value="0.1" />
	</module>


	<module name="planCalcScore">
		<param name="learningRate" value="1.0" />
		<param name="BrainExpBeta" value="2.0" />

		<param name="lateArrival" value="-18" />
		<param name="performing" value="+6" />
		<param name="traveling" value="-6" />
		<param name="earlyDeparture" value="-0" />
		<param name="waiting" value="-0" />
		
		<param name="activityType_0"            value="h" />
		<param name="activityTypicalDuration_0" value="12:00:00" />
	
		<param name="activityType_1"            value="w" />
		<param name="activityTypicalDuration_1" value="08:00:00" />
		
		<param name="activityType_2"            value="school" />
		<param name="activityTypicalDuration_2" value="05:00:00" />
		
		<param name="activityType_3"            value="social" />
		<param name="activityTypicalDuration_3" value="02:00:00" />
		
		<param name="activityType_4"            value="routine" />
		<param name="activityTypicalDuration_4" value="02:00:00" />		
	</module>
	
	<module name="strategy">
		<param name="maxAgentPlanMemorySize" value="5" /> <!-- 0 means unlimited -->

		<param name="ModuleProbability_1" value="0.9" />
		<param name="Module_1" value="BestScore" />

		<param name="ModuleProbability_2" value="0.1" />
		<param name="Module_2" value="ReRoute" />
	</module>
	

</config>
